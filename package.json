{
  "name": "tweet-parser",
  "version": "0.2.0",
  "description": "Parse out entities from tweets.",
  "main": "lib/index.js",
  "author": "Jack Franklin",
  "license": "MIT",
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "babel": "babel src/ -d lib --ignore '**/*.test.js'",
    "check": "flow check && eslint src && jest",
